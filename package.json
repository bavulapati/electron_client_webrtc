{
  "name": "electron_client_webrtc",
  "version": "0.0.1",
  "description": "Electron application whose screen can shared",
  "homepage": "https://github.com/bavulapati/electron_client_webrtc#electron_client_webrtc",
  "bugs": "https://github.com/bavulapati/electron_client_webrtc/issues",
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "main": "release/main.js",
  "scripts": {
    "test": "npm audit fix && npm run lint && npm run transpile && electron .",
    "start": "npm install && npm run transpile && elctron .",
    "lint": "tslint --project tsconfig.json --config tslint.json --fix",
    "tslint": "tslint --config tslint.json --fix",
    "transpile": "tsc --build tsconfig.json",
    "pack": "npm install && npm audit fix && npm run transpile && node ./release/package-app.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run tslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bavulapati/electron_client_webrtc.git"
  },
  "keywords": [
    "electron",
    "webrtc",
    "screen share"
  ],
  "author": "Balakrishna Avulapati <bavulapati@gmail.com> (https://github.com/bavulapati)",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/socket.io-client": "^1.4.32",
    "electron": "^4.2.8",
    "electron-builder": "^20.44.4",
    "tslint": "^5.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "electron-log": "^3.0.6",
    "robotjs": "^0.5.1",
    "socket.io-client": "^2.2.0"
  }
}
